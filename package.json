{
  "name": "now-dotenv",
  "description": "Handy cli tool for managing Zeit Now v2 secrets with dotenv files ðŸ™Œ",
  "repository": "https://github.com/vadistic/now-dotenv",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "version": "0.1.3",
  "license": "MIT",
  "private": false,
  "keywords": [
    "dotenv",
    "env",
    "now",
    "zeit",
    "cli"
  ],
  "bin": {
    "now-dotenv": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "cli": "ts-node -P tsconfig.json src/cli",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "prepare": "yarn lint && yarn format && yarn clean && yarn build"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.7",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
